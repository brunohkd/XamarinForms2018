<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:master="clr-namespace:TipoPaginaXF.TipoPagina.Master"
             x:Class="TipoPaginaXF.TipoPagina.Master.Master">

    <MasterDetailPage.Master>
        <!--Menu-->
        <ContentPage Title="Menu">
            <StackLayout>
                <Button Text="Pagina 1" BackgroundColor="Transparent" Clicked="MudarPagina1"/>
                <Button Text="Pagina 2" BackgroundColor="Transparent" Clicked="MudarPagina2"/>
                <Button Text="Pagina Conteúdo" BackgroundColor="Transparent" Clicked="MudarPaginaConteudo"/>
            </StackLayout>
        </ContentPage>
    </MasterDetailPage.Master>

    <MasterDetailPage.Detail>
        <!--Conteúdo-->
        <master:Conteudo/>
    </MasterDetailPage.Detail>
    
</MasterDetailPage>